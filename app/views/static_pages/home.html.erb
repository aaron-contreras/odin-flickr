<%= form_with url: root_path, method: :get do |f| %>
  <%= f.label :user_id %>
  <%= f.text_field :user_id %>

  <%= f.submit 'Get feed' %>
<% end %>


<% if @photos %>
  <% @photos.each do |photo| %>
    <%= image_tag "https://live.staticflickr.com/#{photo['server']}/#{photo['id']}_#{photo['secret']}_n.jpg" %>
  <% end %>
<% end %>
